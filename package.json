{
  "name": "@dfh-foundation/iso-3166",
  "version": "1.0.0",
  "description": "Fork of wooorm/iso-3166 (country and subdivision codes package)",
  "license": "MIT",
  "keywords": [
    "iso-3166",
    "iso",
    "3166",
    "3166-1",
    "3166-2",
    "3166-3",
    "country",
    "subdivision",
    "revision",
    "province",
    "region",
    "state",
    "territory",
    "code",
    "name",
    "information"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfh-foundation/iso-3166.git"
  },
  "bugs": "https://github.com/dfh-foundation/iso-3166/issues",
  "author": "Johnny Walker <jwalker@designsforhealth.com>",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "types": "dist/main/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@tsconfig/node12": "^1.0.8",
    "@types/hast": "^2.3.1",
    "@types/jest": "^26.0.23",
    "@types/mdast": "^3.0.3",
    "@types/node": "^17.0.1",
    "@types/node-fetch": "^2.5.10",
    "@types/unist": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "concat-stream": "^2.0.0",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "hast-util-select": "^5.0.0",
    "hast-util-to-string": "^1.0.4",
    "hastscript": "^7.0.1",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-each": "^27.0.2",
    "jest-extended": "^0.11.5",
    "lint-staged": "^11.0.0",
    "mdast-util-heading-range": "^3.0.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "p-map": "^5.0.0",
    "prettier": "^2.3.1",
    "rehype-format": "^3.1.0",
    "rehype-parse": "^7.0.1",
    "rehype-stringify": "^8.0.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.4.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "unified": "^9.2.1",
    "unist-builder": "^3.0.0"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc -b",
    "format": "prettier --write '**/*.{js,json,md,mjs,ts}'",
    "lint": "eslint 'scripts/**/*.{js,mjs,ts}' 'src/*.ts'",
    "pregenerate": "rimraf src/generated/**/*.ts",
    "generate": "node scripts/build.mjs && prettier --write 'src/generated/**/*.ts'",
    "prepare": "run-s build",
    "prepublishOnly": "run-s test build",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,mjs,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "engines": {
    "node": ">=12.12.0"
  }
}
